{% extends "ZikulaPLTheme:Layout:" ~ themevars.home_layout ~ ".html.twig" %}
{% block head %}
    {{ parent() }}
    <link href="{{ pageAddAsset('stylesheet', zasset("almasaeed2010/adminlte/dist/css/skins/#{themevars.home_skin}.min.css"), 98)  }}" rel="stylesheet" type="text/css" />
    <link href="{{ pageAddAsset('stylesheet', zasset("almasaeed2010/adminlte/plugins/iCheck/square/blue.css"), 98)  }}" rel="stylesheet" type="text/css" />
{% endblock %} 
{% block body %}
    {{ parent() }}
    <body class="hold-transition {{ themevars.home_layout }} {{ themevars.home_skin }}">
    {% endblock %}
    {% block content %}
        {{ parent() }}
    {% endblock %}
    {% block footer %}
        {{ parent() }}
        <script src="{{ pageAddAsset('javascript', zasset('almasaeed2010/adminlte/plugins/iCheck/icheck.min.js'), 98)  }}"></script>
        <script src="{{ pageAddAsset('javascript', zasset('@ZikulaPLTheme:js/home.js'), 98)  }}"></script>
    {% endblock %}

