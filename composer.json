{
	"name" : "zikula/pl-theme",
	"version" : "1.0.0",
	"description" : "PL Theme based on Zikula Bootstrap theme",
	"type" : "zikula-theme",
	"license" : "LGPL-3.0+",
	"authors" : [{
			"name" : "Kaikmedia",
			"homepage" : "http://kaikmedia.com/"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Zikula\\PLTheme\\" : ""
		}
	},
	"require" : {
		"php" : ">=5.4.1"
	},
	"extra" : {
		"zikula" : {
			"core-compatibility" : ">=1.4.1",
			"class" : "Zikula\\PLTheme\\ZikulaPLTheme",
			"displayname" : "PLTheme",
			"capabilities" : {
				"user" : true,
				"admin" : true
			}
		}
	}
}
